  components: {
    Toast,
    datePicker,
    singlePicker
  },
  data () {
    return {
      show: false,
      toastText: '',
      step: 0,
      sex: [], // 性别
      person: [], // 人物
      family: [
        {name: '自己', index: 0}, {name: '配偶', index: 1}, {name: '儿子', index: 2},
        {name: '女儿', index: 3}, {name: '父亲', index: 4}, {name: '母亲', index: 5},
        {name: '伴侣父亲', index: 6}, {name: '伴侣母亲', index: 7}
